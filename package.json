{
  "name": "sequelize-butler",
  "version": "2.0.8",
  "description": "Useful features to facilitate the use of Sequelize",
  "main": "index.js",
  "types": "main.d.ts",
  "scripts": {
    "build": "npm run build-ts && npm run tslint",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "moment": "^2.25.3",
    "sequelize": "^5.21.8"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.150",
    "@types/node": "^13.13.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "jest": "^26.6.3",
    "mysql2": "^2.1.0",
    "pg": "^8.1.0",
    "sqlite3": "^4.2.0",
    "tedious": "^9.2.3",
    "ts-jest": "^25.5.1",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marco-meini/sequelize-butler.git"
  },
  "keywords": [
    "Sequelize",
    "helper"
  ],
  "author": "Marco Meini",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marco-meini/sequelize-helper/issues"
  },
  "homepage": "https://github.com/marco-meini/sequelize-helper#readme"
}
