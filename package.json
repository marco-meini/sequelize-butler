{
  "name": "sequelize-butler",
  "version": "2.0.7",
  "description": "Useful features to facilitate the use of Sequelize",
  "main": "index.js",
  "types": "main.d.ts",
  "scripts": {
    "build": "npm run build-ts && npm run tslint",
    "build-ts": "tsc",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "test": "jest"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "lodash": "^4.17.12",
    "moment": "^2.24.0",
    "sequelize": "^5.9.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.136",
    "@types/moment": "^2.13.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "jest": "^24.8.0",
    "mysql2": "^1.6.5",
    "pg": "^7.11.0",
    "sqlite3": "^4.0.9",
    "tedious": "^6.2.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.5.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marco-meini/sequelize-butler.git"
  },
  "keywords": [
    "Sequelize",
    "helper"
  ],
  "author": "Marco Meini",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marco-meini/sequelize-helper/issues"
  },
  "homepage": "https://github.com/marco-meini/sequelize-helper#readme"
}
